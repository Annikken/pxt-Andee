{
    "name": "Andee",
    "version": "1.0.1",
    "description": "PXT package to enable the use of the Annikken Andee app on the BBC:MicroBit",
    "license": "MIT",
    "dependencies": {
        "core": "*",
        "bluetooth": "*"
    },
    "files": [
        "README.md",
        "main.ts"
    ],
    "testFiles": [
        "test.ts"
    ],
    "public": true,
    "yotta": {
        "config": {
            "microbit-dal": {
                "bluetooth": {
                    "open": 1,
                    "whitelist": 0
                }
            }
        }
    }
}